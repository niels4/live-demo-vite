{
  "name": "live-demo-vite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --open",
    "generate": "node ./scripts/generate-routes.script.ts",
    "format": "prettier ./src --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest --coverage --project jsdom",
    "test:run": "npm run test:watch -- --run",
    "test:ui": "npm run test:watch -- --ui",
    "test:watch:browser": "vitest --project browser --coverage",
    "test:run:browser": "npm run test:browser:watch -- --run",
    "test": "npm run lint && npm run typecheck && npm run test:run",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "imports": {
    "#src/*": "./src/*",
    "#generated/*": "./generated/*",
    "#test": "./src/test/includes.tsx"
  },
  "dependencies": {
    "jsdom": "27.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.10.0",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.1",
    "@vitest/browser-playwright": "4.0.8",
    "@vitest/coverage-v8": "4.0.8",
    "@vitest/ui": "4.0.8",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3",
    "vite": "npm:rolldown-vite@7.2.2",
    "vite-plugin-websocket-text-relay": "^1.1.0",
    "vitest": "4.0.8",
    "vitest-browser-react": "2.0.2"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  }
}
